{
  "private": true,
  "workspaces": [
    "apps/*",
    "examples/",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-package:react": "turbo run build --filter=react && changeset publish"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "npm@10.1.0"
}